<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../imagen.css" type="text/css">

    <title>LISTA DE LOS DOCTORES </title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
  </head>
<body>
  <%- include('menu') %>
    <div class="card text-center">
        <div class="card-header">
          LISTA
        </div>
        <div class="card-body">
          <h5 class="card-title"> Lista de doctores </h5>
          <p class="card-text">
            
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Cédula</th>
                    <th scope="col">Nombre del doctor</th>
                    <th scope="col">Apellido del doctor</th>
                    <th scope="col">Especialidad</th>
                    <th scope="col">Consultorio</th>
                    <th scope="col">Correo</th>
            
                  </tr>
                </thead>
                <tbody>
                    <% consulta1.forEach(x => { %>

                        <tr>

                            <th scope="row"> <%= x.cedula %> </th>
                            <td><%= x.nombre %></td>
                            <td><%= x.apellido %></td>
                            <td><%= x.especialidad %></td>
                            <td><%= x.consultorio%></td>
                            <td><%= x.correo %></td>
                            <td>
                              
                            </td>
                        </tr>

                        <%
                    }); %>
                  
                </tbody>
              </table>
              <p>
          </p>
          <a href="/crear" class="btn btn-primary">Regresar</a>

        </div>
        <div class="card-footer text-body-secondary">
          DIANA MILENA BERMÚDEZ VALLEJO
        </div>
      </div>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
</html>

